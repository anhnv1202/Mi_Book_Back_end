{
  "name": "book_store_system_back_end",
  "version": "1.0.0",
  "description": "To make it easy for you to get started with GitLab, here's a list of recommended next steps.",
  "main": "app.ts",
  "scripts": {
    "build": "rimraf ./build && tsc && tsc-alias",
    "start": "node build/app.js",
    "start:dev": "nodemon",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migration:generate": "npm run build && ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d ./src/database/data_source.ts ./src/migrations/post-table",
    "migration:run": "npm run build && ts-node ./node_modules/typeorm/cli.js migration:run -d ./build/database/data_source.js",
    "migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js  migration:revert -d ./build/database/data_source.js",
    "seed:config": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "docker:prod": "bash src/script/build.sh prod"
  },
  "keywords": [],
  "author": "",
  "license": "ISC`",
